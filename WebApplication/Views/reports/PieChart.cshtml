@model DotNet.Highcharts.Highcharts

@{
    ViewBag.Title = "Отчет";
}

<h2>Отчет</h2>


<h3>Круговая диаграмма</h3>
<hr />
<dl class="dl-horizontal">
    <dt>Период</dt>
    <dd>
        @ViewBag.Begin - @ViewBag.End
    </dd>

    <dt>Категория</dt>
    <dd>
        @ViewBag.Category
    </dd>
</dl>
<div>
    @(Model)
</div>